    G0 Z[#<_zMove>]

    G0 X[#<_holeX> + #<_pathR> * COS[#<_t> * 360]] Y[#<_holeY> + #<_pathR> * SIN[#<_t> * 360]]
    
    G0 Z[#<_zDepth> + 1]

    O110 if [#<_t> LT #<_tStep>] ; Peck drill first hole.
        G99
        G83 Z0 F#<_zFeedRate> Q0.20 R[#<_zDepth> + 1]
    O110 else
        G1 Z[#<_zDepth>] F#<_zFeedRate>
       
        G1 Z0 F#<_zFeedRate>
        G1 Z[#<_zDepth>] F#<_zFeedRate>
    O110 endif
    
    G0 Z[#<_zMove>]

    #<_t> = [#<_t> + #<_tStep>]

